
    <h4>Pages</h4>

    <table class="page-block-list">
    <% for (id, page) in pages: %>
        <tr>
            <td><a href="/pages/<%= page %>"><%= page %></a></td>
            <td><a href="/pages/editpage/<%= page %>"><button>Edit</button></a></td>
            <td><button class="delete-page" data-name="<%= page %>">Delete</button></td>
        </tr>
    <% end %>
    </table>

    <a href="/pages/editpage"><button>Add Page</button></a>

