
    <h4>Blocks</h4>

    <table class="page-block-list">
    <% for (id, block) in blocks: %>
        <tr>
            <td><span><%= block %></span></td>
            <td><a href="/pages/editblock/<%= block %>"><button>Edit</button></a></td>
            <td><button class="delete-block" data-target="<%= block %>">Delete</button></td>
        </tr>
    <% end %>
    </table>

    <a href="/pages/editblock"><button>Add Block</button></a>

