
    <% for (id, page) in pages: %>
        <div class="page-item" data-name="<%= page %>">
            <a href="/pages/<%= page %>"><%= page %></a>
            <a href="/pages/editpage/<%= page %>"><button>Edit</button></a>
            <button class="delete-page" data-target="<%= page %>">Delete</button>
        </div>
    <% end %>

    <a href="/pages/editpage"><button>Add Page</button></a>

